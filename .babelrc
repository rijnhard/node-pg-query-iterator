{
    "presets": [
        ["env", {
            "targets": {
                "node": "6"
            },
            "useBuiltIns": true
        }],
        "stage-3"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": true,
            "regenerator": true
        }]
    ],
    "env": {
        "testing": {
            "presets": [
                ["env", {
                    "useBuiltIns": true,
                    "modules": "commonjs"
                }]
            ]
        }
    }
}